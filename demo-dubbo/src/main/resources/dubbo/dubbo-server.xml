<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="gatherplatform-dubbo-server" owner="${gatherplatform.dubbo.application.owner}" logger="log4j"/>

    <dubbo:registry address="${gatherplatform.dubbo.registry.address}"/>

    <dubbo:protocol name="dubbo" port="${gatherplatform.dubbo.protocol.port}" threads="200"/>

    <dubbo:monitor protocol="registry" address="${gatherplatform.dubbo.monitor.address}"/>

    <dubbo:provider timeout="20000" loadbalance="roundrobin" retries="0" executes="200"
                    group="${gatherplatform.dubbo.provider.group}" version="${gatherplatform.dubbo.provider.version}"/>

    <!--dubbo服务提供者-->
    <dubbo:service interface="com.xkeshi.eleme.apis.UserDubboService" ref="testDubboServiceImpl" />

</beans>
